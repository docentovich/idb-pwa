{
  "name": "angular16",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start:serve": "ng serve",
    "start:nodemon": "nodemon server/index.js",
    "start:serve:all": "nodemon server | ng serve",
    "//easy-run": "echo 'npm start:dist:all'",
    "start:dist:all": "ng build --prod & npx http-server -p 8081 -c-1 dist/angular16 | nodemon server",
    "start:dist:prod": "ng build --prod & npx http-server -p 8081 -c-1 dist/angular16",
    "start:dist:dev": "ng build --configuration=development & npx http-server -p 8081 -c-1 dist/angular16",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "lint": "ng lint"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~12.2.0",
    "@angular/common": "~12.2.0",
